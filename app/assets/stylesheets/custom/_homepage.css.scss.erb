.banner {
  background-image: linear-gradient(rgba(0, 0, 0, 0.1), rgba(0, 0, 0, 0.1)), url('qayaam2.jpg');
  text-align: left;
  background-color: white;
  background-size: cover;
  background-attachment: scroll;
  background-repeat: no-repeat;
  position: absolute;
  background-position: center;
  height: 650px;
  left: 0;
  right: 0;
  padding-top: 0px;
  opacity: 0.9;
}

.banner h1 {
  font-size: 65px;
  line-height: 66px;
  color: white;
  margin-top: 80px;
  font-family: 'Montserrat', sans-serif;
  opacity: 1;
}

.banner p {
  font-size: 25px;
  line-height: 25px;
  color: white;
  margin-bottom: 45px;
  font-family: 'Montserrat', sans-serif;
}
 
